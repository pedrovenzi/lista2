Table voo as V{
  saida_prev datetime
  chegada_prev datetime
  saida datetime
  chegada datetime
  piloto int [ref: > P.id_piloto]
  aviao int [ref: > A.registro]
  passageiros varchar [ref: > C.CPF_passag]
}

Table piloto as P{
  id_piloto int [pk]
  nome_piloto varchar
  cpf_piloto varchar
  rg_piloto varchar
  dn_piloto date
  email_piloto varchar
  anac_piloto varchar
}

Table aviao as A{
  modelo varchar
  registro varchar [pk]
  quant_assentos int
}

Table passageiros as C{
  nome_passag varchar
  CPF_passag varchar [pk]
  RG_passag varchar
  sexo_passag varchar
  email_passag varchar
  dn_passag datetime
}
